    <eStructuralFeatures xsi:type="ecore:EReference" name="top" eType="#//Field" eOpposite="#//Field/bottom"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bottom" eType="#//Field"
        eOpposite="#//Field/top"/>
