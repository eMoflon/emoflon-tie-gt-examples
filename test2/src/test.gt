import "http://www.eclipse.org/emf/2002/Ecore"
import "platform:/resource/test2/model/Test2.ecore"


pattern matchPartition(boxId:EInt) {
	partition : Partition
	b : Box{
		.type==enum::BIG_BOX
		.name=="testbox"
		.thisBoxId==param::boxId
		.boxSize==0
		.isAssignable==true
		.boxWidthcm==1.1
		-partition->partition
	}
}

rule addPartition{
	b: Box{
		++ -partition->p2
	}
	++p2:Partition
}

rule removePartition{
	p3:Partition
	b:Box{
		-- -partition->p3
	}
}

