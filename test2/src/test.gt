import "http://www.eclipse.org/emf/2002/Ecore"
import "platform:/resource/test2/model/Test2.ecore"


pattern matchPartition() {
	partition : Partition
	b : Box{
		.boxSize==0
		-partition->partition
	}
}

rule addPartition{
	b: Box{
		++ -partition->p2
	}
	++p2:Partition
}

rule removePartition{
	p3:Partition
	b:Box{
		-- -partition->p3
	}
}

