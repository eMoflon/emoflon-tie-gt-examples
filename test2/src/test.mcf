import "platform:/plugin/test2/model/Test2.ecore"
import "http://www.eclipse.org/emf/2002/Ecore"

using "test.gt"

package src.test

class Box {
	
	operation matchAPartition(searchedBoxId:EInt):void{
	    this:Box
		p:Partition
		invoke matchPartition(matchPartition.b = this, matchPartition.partition = p,matchPartition.boxId=searchedBoxId)
	}
	operation removeAPartition():void{
		this:Box
		p:Partition
		invoke removePartition(removePartition.b= this, removePartition.p3 = p)
		
	}
	
	operation addAPartition():void{
		this:Box
		p:Partition
		invoke addPartition(addPartition.b = this,addPartition.p2=p)
	}

}
